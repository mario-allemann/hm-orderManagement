<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Shipping" table="Shipping" lazy="false">
		<id name="trackingId" column="trackingId" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="estimatedDate" column="estimatedDate" type="date" not-null="false" lazy="false"/>
		<property name="effectiveDate" column="effectiveDate" type="date" not-null="false" lazy="false"/>
		<property name="orderId" column="orderId" type="integer" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
